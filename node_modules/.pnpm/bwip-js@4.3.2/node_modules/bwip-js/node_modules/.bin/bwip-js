#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hiramdev/Documentos/barcodes-roque/node_modules/.pnpm/bwip-js@4.3.2/node_modules/bwip-js/bin/node_modules:/home/hiramdev/Documentos/barcodes-roque/node_modules/.pnpm/bwip-js@4.3.2/node_modules/bwip-js/node_modules:/home/hiramdev/Documentos/barcodes-roque/node_modules/.pnpm/bwip-js@4.3.2/node_modules:/home/hiramdev/Documentos/barcodes-roque/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hiramdev/Documentos/barcodes-roque/node_modules/.pnpm/bwip-js@4.3.2/node_modules/bwip-js/bin/node_modules:/home/hiramdev/Documentos/barcodes-roque/node_modules/.pnpm/bwip-js@4.3.2/node_modules/bwip-js/node_modules:/home/hiramdev/Documentos/barcodes-roque/node_modules/.pnpm/bwip-js@4.3.2/node_modules:/home/hiramdev/Documentos/barcodes-roque/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/bwip-js.js" "$@"
else
  exec node  "$basedir/../../bin/bwip-js.js" "$@"
fi
